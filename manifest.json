{
  "manifest_version": 3,
  "name": "AI Tools Hub",
  "version": "1.0",
  "description": "Replace new tab page with a hub of AI tools",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/extension-icon.svg",
    "48": "icons/extension-icon.svg",
    "128": "icons/extension-icon.svg"
  },
  "action": {
    "default_icon": {
      "16": "icons/extension-icon.svg",
      "48": "icons/extension-icon.svg",
      "128": "icons/extension-icon.svg"
    }
  }
}